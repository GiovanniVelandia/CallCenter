package co.com.almundo.callCenter.enumeracion;

public enum EnumEstadoComparendo {

	REGISTRADO(1), //
	VIGENTE(2), //
	PAGADO(3), //
	FINANCIADO(4), //
	IMPUGNADO(5), //
	CONVENIO(6), //
	ANULADO(7), //
	EXONERADO(8), //
	PREFINANCIADO(9), //
	PAGADO_FINANCIACION(10), //
	IMPUGNADO_AXIS(12), //
	COACTIVO(13), //
	ANULADO_FALLO_CONDENATORIO(14), //
	ANULADO_TRASPASO_AXIS(15), //
	ANULADO_AXIS(16), //
	ABSOLUTORIA(17),//
	;

	private int codigo;

	EnumEstadoComparendo(int codigo) {
		this.codigo = codigo;
	}

	public int getCodigo() {
		return codigo;
	}
}
